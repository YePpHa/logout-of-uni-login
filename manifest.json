{
  "manifest_version": 2,
  "name": "Logout of UNI-login",
  "description": "Have you ever wondered if it was possible to log out of UNI-C login without closing your browser? Well, here's your answer.",
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "cookies",
    "notifications",
    "https://login.emu.dk/",
    "https://sso.emu.dk/"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Log out of UNI-C",
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  }
}
